
<div class="row">
  <div class="col s12"> 
    <div class="card horizontal">
      <div class="card-image"></div>
      <div class="card-stacked">
        <h4>Don't Miss These Places For Next Meetup</h4>
        <div class="card-content">
          <p>I am a header</p>
        </div>
        <div class="card-action"><a href="#">This is a link </a></div>
      </div>
    </div>
  </div>
  <div class="col s6"><a href="/groups/new" class="waves-effect waves-light btn">Create Group  </a></div>
  <div class="col s12"><% @groups.each do |group| %>
    <div class="card white">
      <div class="card-content"><span class="card-title"> <b><%= group.name %></b></span>
        <p><%= group.location %></p>
        <p>Next Event</p>
        <p><%= @events[group.id.to_s].name %></p>
        <p><%= @events[group.id.to_s].start_time %></p>
      </div>
      <div class="card-action">
        <p class="white-text">Members</p><% members = @members[group.id.to_s] %>
        <div class="row"></div><% members.each do |member| %>
        <div class="col s1"><% url = member.profile_img %>
          <%= image_tag(url, id: "profile-img") %>
        </div><% end %><a href="/groups/<%= group.id %>">View More... </a>
        <div id="<%= group.id %>" style="width:500px; height:500px; position: relative; overflow: hidden; background-color: rgb(229, 227,223); z-index:0"></div>
      </div>
    </div><% end %>
  </div>
</div>
<script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['G_KEY'] %>&callback=initMap"> </script>